<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cmpe.boun.cultidate">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        >
        <activity android:name="cmpe.boun.cultidate.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="cmpe.boun.cultidate.activity.LoginActivity" android:screenOrientation="portrait" android:label="Login"> </activity>
        <activity android:name="cmpe.boun.cultidate.activity.RegisterActivity" android:screenOrientation="portrait" android:label="Register"> </activity>
        <activity android:name="cmpe.boun.cultidate.activity.ForgotPassActivity" android:screenOrientation="portrait" android:label="ForgotPass"> </activity>
        <activity android:name="cmpe.boun.cultidate.activity.ProfileActivity" android:screenOrientation="portrait" android:label="ProfilePage"> </activity>
        <activity android:name="cmpe.boun.cultidate.activity.SettingsActivity" android:screenOrientation="portrait" android:label="SettingsPage"> </activity>

    </application>

</manifest>